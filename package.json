{
	"name": "portfolio-svelte-",
	"version": "0.0.1",
	"scripts": {
		"fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
		"svelte-dev": "vite dev",
		"dev": "concurrently \"pnpm svelte-dev\" \"pnpm run fastapi-dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.8",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/kit": "^1.28.0",
		"@types/cookie": "^0.6.0",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-headless-table": "^0.17.7",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^4.5.1",
		"vitest": "^1.0.4"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.0",
		"@tanstack/svelte-query": "^5.13.4",
		"@types/node": "^20.10.4",
		"bits-ui": "^0.11.7",
		"clsx": "^2.0.0",
		"concurrently": "^8.2.2",
		"lucide-svelte": "^0.294.0",
		"tailwind-merge": "^2.1.0",
		"tailwind-variants": "^0.1.18"
	}
}